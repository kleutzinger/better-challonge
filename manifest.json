{
"manifest_version": 2,
    "name": "Better Challonge Results",
    "short_name": "challonge+",
    "description": "Improves the results on Challonge bracket pages",
    "version": "0.0.6",
    "icons": {
        "16": "/pics/icon16.png",
        "48": "/pics/icon48.png",
        "128": "/pics/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.challonge.com/*"
            ],
            "js": [
                "jquery.js","tablemaker.js","main.js"
            ],
            "run_at": "document_end"
        }
    ]
}
